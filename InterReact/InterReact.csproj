<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProject>https://github.com/dshe/InterReact</PackageProject>
    <PackageIcon>worm64.png</PackageIcon>
    <PackageTags>IB Interactive-Brokers IBApi Trader-Workstation TWS reactive reactive-extensions Rx</PackageTags>
    <Description>Interactive Brokers reactive C# API.</Description>
    <Authors>DavidS</Authors>
    <Version>0.1.3</Version>
    <AssemblyVersion>0.1.3</AssemblyVersion>
    <FileVersion>0.1.3</FileVersion>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
	<EnablePackageValidation>true</EnablePackageValidation>
	<AnalysisMode>All</AnalysisMode>
	<NoWarn>CA1848;</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Exts\Union.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
    <PackageReference Include="NodaTime" Version="3.1.4" />
    <PackageReference Include="RxSockets" Version="4.0.5" />
    <PackageReference Include="Stringification" Version="3.0.3" />
    <PackageReference Include="System.Reactive" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="worm64.png" Pack="true" PackagePath="\" />
  </ItemGroup>


</Project>
