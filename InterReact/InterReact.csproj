<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <PackageProject>https://github.com/dshe/InterReact</PackageProject>
    <PackageIcon>worm64.png</PackageIcon>
    <PackageTags>IB Interactive-Brokers Trader-Workstation TWS reactive reactive-extensions Rx</PackageTags>
    <Description>Interactive Brokers reactive C# API.</Description>
    <Authors>DavidS</Authors>
    <Version>0.0.1-alpha</Version>
    <AssemblyVersion>0.0.1</AssemblyVersion>
    <FileVersion>0.0.1</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <DebugType>embedded</DebugType>
    <DebugSymbols>true</DebugSymbols>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Extensions\CollectionBinarySearcherExtensions.cs" />
    <Compile Remove="~\AccountObservableCollections.cs" />
    <Compile Remove="~\DistinctByExtension.cs" />
    <Compile Remove="~\MarketDepthObservableCollections.cs" />
    <Compile Remove="~\NotifyPropertyChanged.cs" />
    <Compile Remove="~\OrderMonitor.cs" />
    <Compile Remove="~\ReadOnlyObservableList.cs" />
    <Compile Remove="~\ToObservableCollectionEx.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="5.0.0" />
    <PackageReference Include="NodaTime" Version="3.0.5" />
    <PackageReference Include="RxSockets" Version="3.0.1" />
    <PackageReference Include="StringEnums" Version="2.0.0-beta" />
    <PackageReference Include="Stringification" Version="2.0.0-beta" />
    <PackageReference Include="System.Reactive" Version="5.0.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="~\CollectionBinarySearcherExtensions.cs" />
    <None Include="worm64.png" Pack="true" PackagePath="\" />
    <None Include="~\AccountObservableCollections.cs" />
    <None Include="~\DistinctByExtension.cs" />
    <None Include="~\MarketDepthObservableCollections.cs" />
    <None Include="~\NotifyPropertyChanged.cs" />
    <None Include="~\OrderMonitor.cs" />
    <None Include="~\ReadOnlyObservableList.cs" />
    <None Include="~\ToObservableCollectionEx.cs" />
  </ItemGroup>

</Project>
